void pay_amount(int dollars, int *twenties, int *tens,
                int *fives, int *ones) {
  *twenties = dollars / 20;
  printf("%d", *twenties);
  dollars -= *twenties * 20;
  *tens = dollars / 10;
  printf("%d", *tens);
  dollars -= *tens * 10;
  *fives = dollars / 5;
  printf("%d", *fives);
  dollars -= *fives * 5;
  *ones = dollars / 1;
  printf("%d", *ones);
  
  return 0;
  }
